<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/gray_dark_transparent"
    tools:context="com.john.Fragments.PessoaFisicaFragment">

    <!-- TODO: Update blank fragment layout -->

        <ListView
            android:id="@+id/list_Pjuridica"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:descendantFocusability= "beforeDescendants"
            android:focusableInTouchMode="false">

        </ListView>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.melnykov.fab.FloatingActionButton
                android:id="@+id/fabJuridica"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="bottom|right"
                android:layout_margin="16dp"
                android:src="@drawable/done"
                app:fab_colorNormal="@color/orange"
                app:fab_colorPressed="@color/orange_very_light"
                app:fab_colorRipple="@color/orange_light"
                android:onClick="PJuridicaButtonClick"/>
        </FrameLayout>


</FrameLayout>